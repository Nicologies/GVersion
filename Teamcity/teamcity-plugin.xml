<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>GVersion</name>
    <display-name>A simplified version of GitVersion</display-name>
    <version>@Version@</version>
    <description>Versioning when using git. GVersion looks at your git history and works out the semantic version (semver.org) of the commit being built.</description>
    <download-url>https://github.com/Nicologies/GVersion/releases/latest</download-url>
    <email>nicologies@gmail.com</email>
    <vendor>
      <name>Nicologies</name>
      <url>https://github.com/Nicologies/GVersion/releases/latest</url>
      <logo>Plugin vendor logo URL</logo>
    </vendor>
  </info>
  <deployment use-separate-classloader="true" />
</teamcity-plugin>
